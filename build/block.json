{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "b-blocks/lorem-ipsum",
  "version": "1.0.0",
  "title": "Lorem Ipsum",
  "category": "widgets",
  "description": "Use Emmet like abbreviations to quickly create dummy content in Gutenberg. supports custom lorem word counts and block replacement.",
  "keywords": [
    "lorem",
    "ipsum"
  ],
  "textdomain": "b-blocks",
  "attributes": {
    "align": {
      "type": "string",
      "default": ""
    },
    "isBlockReplace": {
      "type": "boolean",
      "default": true
    },
    "content": {
      "type": "string",
      "default": ""
    },
    "loremText": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ],
    "html": false
  },
  "example": {
    "attributes": {}
  },
  "editorScript": "file:./index.js"
}